        <!-- 引导页 -->
        <activity
            android:name=".ui.activity.GuideActivity"
            android:screenOrientation="portrait" />

        <!-- 登录kotlin——mvp -->
        <activity
            android:name=".ui.activity.LoginActivity"
            android:screenOrientation="portrait" />

        <!-- 测试java-mvp -->
        <activity
            android:name=".ui.activity.TestMvpActivity"
            android:screenOrientation="portrait" />
        <!-- 测试java-mvp -->
        <activity
            android:name=".ui.activity.ListViewActivity"
            android:screenOrientation="portrait" />
